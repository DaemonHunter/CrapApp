{
  "expo": {
    "name": "CrapApp",
    "slug": "crapapp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/png/logo-no-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daemonhunter.crapapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/logo-no-background.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.daemonhunter.crapapp",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/png/logo-no-background.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "97776c8f-21fe-4c15-ac48-c8ad074bb8b0"
      }
    },
    "owner": "daemonhunter",
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
